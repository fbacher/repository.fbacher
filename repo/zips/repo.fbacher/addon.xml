<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="repo.fbacher" name="fbacher Repository" version="1.0.0"
       provider-name="fbacher">
   <requires>
      <!-- <import addon="xbmc.addon" version="20.0.0"/> -->
   </requires>
   <extension point="xbmc.addon.repository" name="fbacher Repository">
      <dir minversion="20.0.0">
         <!-- DO NOT add extra whitespace to the element or attribute values. -->
         <info compressed="false">http://smeagol/repository.fbacher/repo/zips/addons.xml.gz</info>
         <!-- Without the verify attribute, no validation of the hash is made.
              Also, the hash is performed on the addons.xml.gz file. You can put the hahs
              in the following line in either addons.xml.gz.md5 or addons.xml.md5. The file name
              is not paid much attention to (not tested).-->
         <checksum verify="md5">http://smeagol/repository.fbacher/repo/zips/addons.xml.gz.md5</checksum>
         <datadir zip="true">http://smeagol/repository.fbacher/repo/zips/</datadir>
         <hashes>sha256</hashes>
      </dir>
      <!-- <dir>
         <info compressed="false">
            http://raw.githubusercontent.com/fbacher/repository.fbacher/master/repo/zips/addons.xml
         </info>

         <checksum verify="md5">http://raw.githubusercontent.com/fbacher/repository.fbacher/master/repo/zips/addons.xml.md5</checksum>
         <!- - <info>
            http://smeagol/repository.fbacher/repo/zips/addons.xml.gz
         </info> - ->
         <hashes>false</hashes>
         <datadir zip="true">
            http://raw.githubusercontent.com/fbacher/repository.fbacher/master/repo/zips/
         </datadir>
      </dir> -->
   </extension>
   <extension point="xbmc.addon.metadata">
      <summary>fbacher Repository</summary>
      <description>Repo for Kodi addon projects and needed dependencies</description>
      <disclaimer></disclaimer>
      <platform>all</platform>
      <assets>
         <icon>icon.png</icon>
         <!-- <fanart>fanart.jpg</fanart> -->
      </assets>
   </extension>
</addon>
