   </extension>
   <extension point="xbmc.addon.metadata">
      <summary>fbacher Repository</summary>
      <description>Repo for Kodi addon projects and needed dependencies</description>
      <disclaimer></disclaimer>
      <platform>all</platform>
      <assets>
         <icon>icon.png</icon>
         <!-- <fanart>fanart.jpg</fanart> -->
      </assets>
   </extension>
</addon>
